
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>登录表单</title>
    <script src="~/Content/Manage/js/jquery-3.6.0.min.js"></script>
    <link href="~/Content/Manage/css/style.css" rel="stylesheet" />
    <link href="~/Content/Manage/css/css.css" rel="stylesheet" />
    <link href="~/Content/Manage/layui/css/layui.css" rel="stylesheet" />
    <script src="~/Content/Manage/layui/layui.js"></script>
    <script src="~/Content/Manage/js/a81368914c.js"></script>
    <!-- https://fontawesome.com/v5/search?s=solid%2Cbrands -->
</head>
<body>
    <img src="~/Content/Manage/img/wave.png" class="wave">
    <div class="container">
        <div class="img">
            <img src="~/Content/Manage/img/bg.svg" alt="">
        </div>
        <div class="login-content">
            <form action="index.html">
                <img src="~/Content/Manage/img/avatar.svg" alt="">
                <h2 class="title">管理员登录</h2>
                <div class="input-div one">
                    <div class="i">
                        <i class="fas fa-user"></i>
                    </div>
                    <div class="div">
                        <h5>用户名</h5>
                        <input type="text" class="input" id="UserName" autocomplete="off" value="" />
                    </div>
                </div>
                <div class="input-div pass">
                    <div class="i">
                        <i class="fas fa-lock"></i>
                    </div>
                    <div class="div">
                        <h5>密码</h5>
                        <input type="password" class="input" id="Password" value="" />
                    </div>
                </div>
                <input type="button" class="btn" id="Login" value="登录" />
            </form>
        </div>
    </div>
    <script src="~/Content/Manage/js/Login.js"></script>
</body>
</html>
<script src="~/Content/Manage/js/LoginButton.js"></script>